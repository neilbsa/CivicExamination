@model CivicExamination.Models.ScheduleExaminerViewModel



<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3">
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xs-12">
                <div class="thumbnail">
                   <div class="caption">
                        @Html.ActionLink("Back To List", "ViewResult", "Schedule", new { schedId = Model.Id }, new { @class = "btn btn-primary" })
                    </div>
                </div>
            </div>
        </div>    
    </div>
</div>
<div class="row">
    @Html.HiddenFor(x => x.Id, new { Id = "ScheduleId" })
    @Html.HiddenFor(x => x.UserId, new { Id = "userSchedId" })
    <div class="col-xs-12 col-sm-12 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3">
        @foreach (var item in Model.CorrectedQuestion)
        {
            Html.RenderPartial("UpdateEssayReadOnly", item);
        }
    </div>
</div>


@Scripts.Render("~/bundles/modernizr")
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/MainExam")