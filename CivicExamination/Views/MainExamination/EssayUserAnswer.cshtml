@model  ExaminationEntity.ExaminationModels.UserAnswer
@{
    Layout = null;
}

<div class="essayAnswer mdl-textfield mdl-js-textfield mdl-textfield--floating-label" data-choiceId=@Model.Id data-quesId=@Model.ScheduleQuestionId style="width:100%;">
    @Html.TextAreaFor(model => model.ChoiceString, 5, 5, new { style = "width:100%;height:100px;", maxlength = 355, @class = "userAnswer mdl-textfield__input" })
    @Html.Label("Answer", new { @class = "mdl-textfield__label" })
</div>
