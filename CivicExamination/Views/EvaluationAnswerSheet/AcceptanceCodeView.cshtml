@{
    Layout = null;
}



@using (Html.BeginForm("UserAccept", "EvaluationAnswerSheet", FormMethod.Post, new { id = "acceptanceCodeView" }))


{
    @Html.AntiForgeryToken()

    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <center>
                <p>
                   Please input Acceptance code.
                </p>
            </center>
        </div>
    </div>

    <div class="row">
        <div class="col-xs-6 col-xs-offset-3 col-sm-6 col-sm-offset-3 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3">
            <center>
                <input type="text" value="@ViewBag.AnsId" readonly="readonly" name="AnsId" class="form-control" />
            </center>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-6 col-xs-offset-3 col-sm-6 col-sm-offset-3 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3" placeholder="User Acceptance Code">
            <center>
                @Html.TextBox("AcceptanceCode", null, new { @class = "form-control" })
            </center>
        </div>
    </div>




}



