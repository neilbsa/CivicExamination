@model ExaminationEntity.ExaminationModels.Category

<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-lg-offset-3">

        <div class="panel panel-default">
            <div class="panel-heading">Category Details</div>
            <div class="panel-body">
                <div class="row">

                    <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">

                            @Html.TextBoxFor(p => p.CategoryName, new { @class = "mdl-textfield__input", @readonly="readonly" })
                            @Html.Label("Category Name", new { @class = "mdl-textfield__label" })
                      
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                        <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                            @Html.TextBoxFor(p => p.CreateUser, new { @class = "mdl-textfield__input", @readonly = "readonly" })
                            @Html.Label("Created By", new { @class = "mdl-textfield__label" })
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-default">
            <div class="panel-heading">Question List</div>
            <div class="panel-body">
              
            </div>
        </div>



    </div>
</div>
